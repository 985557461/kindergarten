<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--ViewFlow begin-->
    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CircleFlowIndicator">
        <attr name="fillColor2" format="color"/>
        <attr name="strokeColor2" format="color"/>
        <attr name="radius2" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TitleFlowIndicator">
        <attr name="titlePadding2" format="integer"/>
        <attr name="selectedColor2" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="float"/>
        <attr name="footerLineHeight2" format="integer"/>
        <attr name="footerColor2" format="color"/>
        <attr name="footerTriangleHeight2" format="integer"/>
    </declare-styleable>
    <!--viewFlow end-->

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="border_overlay" format="boolean"/>
    </declare-styleable>

    <!--roundImageView begin-->
    <attr name="borderRadius" format="dimension"/>
    <attr name="type">
        <enum name="circle" value="0"/>
        <enum name="round" value="1"/>
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius"/>
        <attr name="type"/>
    </declare-styleable>
    <!--roundImageView end-->

    <!--RoundPartImageView begin-->
    <declare-styleable name="RoundPartImageView">
        <attr name="roundWidth" format="dimension"/>
        <attr name="roundHeight" format="dimension"/>
        <attr name="leftTopRound" format="boolean"/>
        <attr name="leftBottomRound" format="boolean"/>
        <attr name="rightTopRound" format="boolean"/>
        <attr name="rightBottomRound" format="boolean"/>
    </declare-styleable>
    <!--RoundPartImageView end-->

    <item name="id_stickynavlayout_topview" type="id"/>
    <item name="id_stickynavlayout_indicator" type="id"/>
    <item name="id_stickynavlayout_viewpager" type="id"/>
    <item name="id_stickynavlayout_innerscrollview" type="id"/>
</resources>